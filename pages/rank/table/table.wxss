.rank-table {
  width: 100%;
  height: 755rpx;
}

.rank-table-item {
  float: left;
  height: 120rpx;
  line-height: 120rpx;
  border-bottom: 1px solid #ccc;
  text-align: center;
  color: #444;
  font-weight: bold;
}

.rank-table-item>view {
  display: inline-block;
}

.col-2 {
  width: 16.66666666666667%;
}

.col-4 {
  width: 33.33333333333333%;
}
